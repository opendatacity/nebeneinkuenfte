						</tbody>
					</table>
				</article>
				<article id="mandate">
					<h2>Bundesländer</h2>
					<table class="table table-striped table-bordered table-condensed table-responsive">
						<thead>
							<tr>
								<td rowspan="2">Bundesland</td>
								<td rowspan="2" class="digit">Abgeordnete</td>
								<td rowspan="2" class="digit">mit Nebeneinkünften</td>
								<td rowspan="2" class="digit">%</td>
								<td rowspan="2" class="currency">Gesamtbetrag (Minimum)</td>
								<td rowspan="2" class="currency">Durchschnitt (Minimum)</td>
								<td colspan="10">Stufen</td>
							</tr>
							<tr>
								<td class="c">1</td>
								<td class="c">2</td>
								<td class="c">3</td>
								<td class="c">4</td>
								<td class="c">5</td>
								<td class="c">6</td>
								<td class="c">7</td>
								<td class="c">8</td>
								<td class="c">9</td>
								<td class="c">10</td>
							</tr>
						</thead>
						<tbody>
							{{#laender}}
							<tr>
								<td>{{name}}</td>
								<td class="n">{{members}}</td>
								<td class="n">{{earners}}</td>
								<td class="n">{{#percent}}{{ratio_earners_per_member}}{{/percent}}</td>
								<td class="n">{{#total}}{{min}}{{/total}}</td>
								<td class="n">{{#total}}{{ratio_min_per_member}}{{/total}}</td>
								{{{format_map}}}
							</tr>
							{{/laender}}
						</tbody>
					</table>
				</article>
				<article id="mandate">
					<h2>Mandate</h2>
					<table class="table table-striped table-bordered table-condensed table-responsive">
						<thead>
							<tr>
								<td rowspan="2">Mandat</td>
								<td rowspan="2" class="digit">Abgeordnete</td>
								<td rowspan="2" class="digit">mit Nebeneinkünften</td>
								<td rowspan="2" class="digit">%</td>
								<td rowspan="2" class="currency">Gesamtbetrag (Minimum)</td>
								<td rowspan="2" class="currency">Durchschnitt (Minimum)</td>
								<td colspan="10">Stufen</td>
							</tr>
							<tr>
								<td class="c">1</td>
								<td class="c">2</td>
								<td class="c">3</td>
								<td class="c">4</td>
								<td class="c">5</td>
								<td class="c">6</td>
								<td class="c">7</td>
